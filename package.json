{
  "name": "etudeplayer",
  "version": "0.7.1",
  "description": "Etude Management Media Center",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dist": "electron-builder --dir"
  },
  "keywords": [],
  "author": "HostAngelDoll",
  "license": "ISC",
  "dependencies": {
    "chokidar": "^4.0.3",
    "ffmpeg-peaks": "^0.3.3",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0"
  },
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-debug": "^4.1.0"
  },
  "build": {
    "appId": "com.hostangeldoll.etudeplayer",
    "productName": "EtudePlayer",
    "files": [
      "!dist/",
      "!node_modules/.cache",
      "!.venv/**",
      "!.vscode/**",
      "assets/**/*",
      "css/**/*",
      "!docs/**/*",
      "fonts/**/*",
      "html/**/*",
      "js/**/*",
      "json/**",
      "!private/",
      "!test/**/*",
      "!.gitignore",
      "!LICENCE",
      "main.js",
      "package-lock.json",
      "package.json",
      "!README.md"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "assets/icon_tb.ico",
      "target": "nsis"
    }
  }
}